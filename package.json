{
  "name": "thoth-lambda",
  "version": "1.6.2",
  "description": "A authentication microservice",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "scripts": {
    "t-init": "terraform -chdir=./terraform init",
    "t-reconfigure": "terraform -chdir=./terraform init -reconfigure",
    "t-plan": "terraform -chdir=./terraform plan -var-file=\"config/config.tfvars\" -var-file=\"config/config.tfvars.secret\"",
    "t-fmt": "terraform -chdir=./terraform fmt",
    "bundle": "node ./bin/bundle.js",
    "prebundle": "yarn build",
    "build": "rollup --config config/rollup.es.js",
    "ship": "terraform -chdir=./terraform apply -var-file=\"config/config.tfvars\" -var-file=\"config/config.tfvars.secret\"",
    "dev": "nodemon --ignore dist --exec npm run build",
    "dev-start": "node src/dev.js",
    "lint-scripts": "eslint \"./@(lib|bin)/**/*.js?(x)\"",
    "test": "nyc mocha $(find tests -name '*.js') --require \"@babel/register\" --timeout 30000",
    "test-ci": "yarn test > test-results.txt"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.4.2",
    "@politico/eslint-config-interactives": "^1.0.1",
    "archiver": "^5.3.0",
    "aws-sdk": "^2.659.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.19.0",
    "eslint-plugin-mocha": "^8.1.0",
    "expect.js": "^0.3.1",
    "express": "^4.17.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rollup": "^1.7.3",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-preserve-shebang": "^0.1.6",
    "yarn": "^1.9.4"
  },
  "dependencies": {
    "@notionhq/client": "^1.0.4",
    "@slack/web-api": "^6.2.4",
    "dotenv": "^8.2.0",
    "ellipsize": "^0.1.0",
    "fs-extra": "^8.1.0",
    "gootenberg": "^1.6.2",
    "isomorphic-unfetch": "^3.0.0",
    "journalize": "^2.5.1",
    "lodash": "^4.17.14"
  }
}
